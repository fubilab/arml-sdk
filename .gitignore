*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
sysinfo.txt

# Builds
*.apk
*.aab
*.unitypackage
*.app

# Crashlytics generated file
crashlytics-build.properties

# Packed Addressables
/unity-arml-sdk/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/unity-arml-sdk/[Aa]ssets/[Ss]treamingAssets/aa.meta
/unity-arml-sdk/[Aa]ssets/[Ss]treamingAssets/aa/*

# Mac 
.DS_Store

